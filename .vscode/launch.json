    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Attach to packager",
            "program": "${workspaceRoot}\\src\\App.js",
            "type": "reactnative",
            "request": "attach",
            "sourceMaps": true,
            "outDir": "${workspaceRoot}/index.js",
            "host":"192.168.1.49",
            "port":"8081"
        },
        {
            "name": "Debug Android",
            "program": "${workspaceRoot}\\src\\App.js",
            "type": "reactnative",
            "request": "launch",
            "platform": "android",
            "sourceMaps": true,
            "outDir": "${workspaceRoot}/index.js",
           
        },

        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}\\start"
        }
    ]
}